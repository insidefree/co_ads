<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="robots" content="noindex">
    <meta>
    <title>Google AdSense App</title>
</head>
<body>

<!-- adsense advertisement -->
<div class="drive_widget_main">
    <!-- adsense code -->
    {#{{ code|raw }}#}

    <!-- adsense customization -->
    <script type="text/javascript">
        google_ad_client = "ca-pub-5195028014239031";
//        google_ad_slot = '';

        google_ad_width = {{ 300 }};
        google_ad_height = {{ 600 }};

        google_ad_format = '300x600_as';

//        google_alternate_ad_url = "http://www.ynet.co.il";

        google_ad_type = '{{ 'text' }}';

        google_font_face = "verdana";
        google_font_size = 'small';

        google_color_border = '{{ '00000' }}';
        google_color_bg = '{{ 'cccccc' }}';
        google_color_link = '{{ 'cccccc' }}';
        google_color_url = '{{ 'ffffff' }}';
        google_color_text = '{{ '000000' }}';
        google_ui_features = '{{ 'rc:10' }}';
    </script>

    <script type="text/javascript"
            src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
    </script>
</div>

</body>
</html>