<!doctype html>
<html lang="en" data-ng-app="adSenseApp">
<head>
    <meta charset="utf-8">
    <meta name="robots" content="noindex, nofollow">
    <title>Google AdSense Settings</title>
    {% stylesheets filter='yui_css, cssrewrite'
    '@WixGoogleAdSenseAppBundle/Resources/public/app/lib/bootstrap/css/bootstrap.css'
    '@WixGoogleAdSenseAppBundle/Resources/public/app/lib/color-picker/color-picker.css'
    '@WixGoogleAdSenseAppBundle/Resources/public/app/lib/chosen/chosen.css'
    '@WixGoogleAdSenseAppBundle/Resources/public/app/css/emphasize.css'
    '@WixGoogleAdSenseAppBundle/Resources/public/app/css/master.css'
    '@WixGoogleAdSenseAppBundle/Resources/public/app/css/settings.css' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
</head>
<body>

<!-- container for the adsense app -->
<div data-ng-view></div>

<!-- wix sdk -->
<script type="text/javascript" src="//sslstatic.wix.com/services/js-sdk/1.16.0/js/Wix.js"></script>

<!-- fos js routing bundle -->
<script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
<script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>

<!-- scripts -->
{% javascripts
'@WixGoogleAdSenseAppBundle/Resources/public/app/lib/jquery/jquery.min.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/lib/bootstrap/js/bootstrap.min.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/lib/color-picker/color-pickers/simple.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/lib/color-picker/color-pickers/advanced.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/lib/color-picker/color-picker.js' %}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}

{% javascripts filter='closure'
'@WixGoogleAdSenseAppBundle/Resources/public/app/lib/jquery-ui/js/jquery-ui.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/lib/chosen/chosen.jquery.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/lib/angular/angular.min.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/js/jquery-ui.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/js/app.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/js/services.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/js/filters.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/js/directives.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/js/ajax-events.js'
'@WixGoogleAdSenseAppBundle/Resources/public/app/js/controllers/settings.js' %}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
</body>
</html>