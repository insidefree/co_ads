<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="robots" content="noindex, nofollow">
    <meta>
    <title>Google AdSense Widget</title>
</head>
<body>

    <!-- adsense advertisement -->
    <div style="text-align: center">
        {% if code is defined and app.request.get('viewMode') != 'editor' %}
            {% include 'WixGoogleAdsenseBundle:View:ad.html.twig' %}
        {% else %}
            {% include 'WixGoogleAdsenseBundle:View:demo.html.twig' %}
        {% endif %}
    </div>

    <!-- wix sdk -->
    <script type="text/javascript" src="https://sslstatic.wix.com/services/js-sdk/1.44.0/js/Wix.js"></script>
    <script>
        (function(i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function() {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-47138354-1', 'wix.com');
        ga('send', 'pageview');

    </script>
    <script src="//code.jquery.com/jquery-1.10.1.min.js"></script>

    {% if code is not defined %}
        <!-- fos js routing bundle -->
        <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>

        <!-- scripts -->
        {% javascripts
        '@WixGoogleAdsenseBundle/Resources/public/lib/jquery/jquery.js'
        '@WixGoogleAdsenseBundle/Resources/public/js/responsive.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endif %}
</body>
</html>
